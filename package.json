{
    "name": "towerstats.js",
    "type": "module",
    "version": "1.0.2",
    "description": "A JavaScript API wrapper for the TowerStats.com API.",
    "devDependencies": {
        "@types/bun": "latest",
        "del-cli": "^6.0.0",
        "prettier": "^3.6.2",
        "typedoc": "^0.28.13",
        "typedoc-github-theme": "^0.3.1",
        "typescript": "^5.9.3"
    },
    "scripts": {
        "build": "bun x tsc",
        "format": "bun x prettier --write .",
        "docs": "bun x typedoc --plugin typedoc-github-theme --cname towerstats.js.lovelyjacob.com",
        "bundle": "(bun build src/index.ts > dist/bundle.js)",
        "compile": "del dist --verbose && del docs --verbose && bun x tsc && bun x typedoc --plugin typedoc-github-theme --cname towerstats.js.lovelyjacob.com && bun x prettier --write . && (bun build src/index.ts > dist/bundle.js)"
    },
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jacobhumston/towerstats.js.git"
    },
    "maintainers": [
        {
            "name": "Jacob Humston",
            "email": "hello@lovelyjacob.com"
        }
    ],
    "files": [
        "dist/",
        "LICENSE",
        "package.json",
        "README.md"
    ],
    "author": "jacobhumston",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jacobhumston/towerstats.js/issues"
    }
}
